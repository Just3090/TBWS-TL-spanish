# game/day_2.rpy:1137
translate spanish day_2_9cd2463c:

    # m "Are you crying? {w=0.4}Over that!?"
    m "¿Estás llorando? {w=0.4}¿¡Por eso!?"

# game/day_2.rpy:1138
translate spanish day_2_2775dbbb:

    # m "Men don't cry!"
    m "¡Los hombres no lloran!"

# game/day_2.rpy:1139
translate spanish day_2_a545594f:

