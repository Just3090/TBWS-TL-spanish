translate spanish day_2_5cd7ea9f:

    # li "Sayori. How lovely to see you."
    li "Sayori. Qué encanto verte."

# game/day_2.rpy:106
translate spanish day_2_4bd9b9a3:

    # "S-sayori?"
    "¿S-Sayori?"

